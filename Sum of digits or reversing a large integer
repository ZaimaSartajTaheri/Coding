#include <iostream>
using namespace std;
int main() {
  int x;
  cin>>x;
//   int sum=0;
  int reversed=0;
  while(x>0){
     int digit=x%10;
     x/=10;
    // 
     reversed=reversed*10+digit;
  }
//   cout<<sum<<endl;
  cout<<reversed<<endl;
    return 0;
}
for reversing
input: 12345
output: 54321

for sum of digits
input: 12345
output: 15
