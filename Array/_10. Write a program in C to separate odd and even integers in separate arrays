#include <stdio.h>

int main()
{
    int n;
    printf("Input the number of elements to be stored in the array : ");
    scanf( "%d",&n);
    printf("Input %d elements in the array :",n);
    printf("\n");
    int arr[n];
    
    for(int i=0;i<n;i++){
        printf("element - %d: ",i);
        scanf("%d",&arr[i]);
    }
   printf("The Even elements are :\n");
    for(int i=0;i<n;i++){
        if(arr[i]%2==0){
            printf("%d  ",arr[i]);
        }
    }
   
    printf("\nThe Odd elements are :\n");
    
    for(int i=0;i<n;i++){
        if(arr[i]%2!=0){
             printf("%d  ",arr[i]);
        }
    }
    
    return 0;
}
