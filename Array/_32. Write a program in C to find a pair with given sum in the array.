#include <stdio.h>

int main()
{ int n;
scanf("%d",&n);
int a[n];
for(int i=0;i<n;i++){
    scanf("%d",&a[i]);
}
int m;
scanf("%d",&m);
for(int i=0;i<n;i++){
    for(int j=i+1;j<n;j++){
        if(a[i]+a[j]==m){
            printf("Pair of elements can make the given sum by the value of index %d and %d\n",i,j);
            return 0;
        }
    }
   
}

    return 0;
}
