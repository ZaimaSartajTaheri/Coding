#include <bits/stdc++.h>
using namespace std;
int main()
{
   int a[9]={4,8,7,11,6,9,5,0,2};
    int b[5]={5,4,2,0,6};
    int c=0;
   for(int i=0;i<5;i++){
       for(int j=0;j<9;j++){
           if(b[i]==a[j]){
               c++;
           }
       }
   }
   if(c==5){
       cout<<"The second array is the subset of first array"<<endl;
   }
   else{
       cout<<"The second array is not the subset of first array"<<endl;
   }

    return 0;
}
