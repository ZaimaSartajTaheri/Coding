#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    int c[6]={0};
    for(int i=0;i<n;i++){
      cin>>a[i];
    }
    for(int i=0;i<n;i++){
        c[a[i]]++;
    }
    for(int i=0;i<6;i++){
        cout<<c[i]<<" ";
    }
    cout<<endl;
     return 0;   
}
