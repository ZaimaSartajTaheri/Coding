#include <stdio.h>

int main() {
    double var=2.0;;
    printf("%p %lld\n",&var,&var);
    double var2;
    printf("%p %lld\n",&var2,&var2);
     
    double *pvar=&var;
    printf("%p %lld\n",pvar,pvar);   
    pvar+=10; //1 kore barale 8 ghor bare. 10 kore barano mane 80 ghor barbe
     printf("%p %lld\n",pvar,pvar); 
    
    return 0;
}
