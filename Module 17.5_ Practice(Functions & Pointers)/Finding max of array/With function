#include <stdio.h>
int max(int a,int b){
    if(a>b){
        return a;
    }
    else{
        return b;
    }
}
int getMax(int n, int a[]){
   int ans=a[0] ;
   for(int i=0;i<n;i++){
       ans=max(ans,a[i]);
   }
   return ans;
}
int main() {
    // Write C code here
   int n;
   scanf("%d",&n);
   int a[n];
   for(int i=0;i<n;i++){
       scanf("%d",&a[i]);
   }
   int max;
   max=getMax(n,a);
   printf("%d",max);
    
    return 0;
}
