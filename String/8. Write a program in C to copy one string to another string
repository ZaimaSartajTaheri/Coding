#include <stdio.h>
#include<string.h>
int main() {
 char c[100];
 char d[100];
 printf("The First string is : ");
gets(c);
int m;
int n=0;
for(int i=0;c[i]!='\0';i++){
    d[i]=c[i];
    m=i;
    n++;
}
 c[m]='\0';
 printf("The Second string is : %s\n",d);
 printf("Number of characters copied : %d\n",n+1);
    return 0;
}
